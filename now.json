{
	"version": 2,
	"routes": [
		{ "handle": "filesystem" },
		{
			"src": "/vbadge/(.*)/(.*)",
			"dest": "https://img.shields.io/badge/dynamic/json?color=$2&label=$1&prefix=v&query=%24%5B%27dist-tags%27%5D.latest&url=https%3A%2F%2Fregistry.npmjs.com%2F$1"
		},
		{ "src": "/(.*)", "dest": "/pages/$1" }
	]
}
